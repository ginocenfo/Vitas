entity Hospital {
	nombre 					String
    provincia 				String
    canton 					String
    distrito 				String
    direccion 				String
    telefono 				String
    email 					String
}

entity Sala {
	nombre 					String
    numero					String
    piso					Integer
    totalPacientes			Integer
    numPacientes			Integer
    numVisitantes			Integer
    visitaDisponible		Boolean
}

entity Horario{
	horaEntrada				Instant
    horaSalida				Instant
}

entity Usuario{
	identidad		    	Long
    primerNombre			String
    segundoNombre			String
    primerApellido			String
    segundoApellido			String
    fechaNacimiento			LocalDate
    paisNacimiento			String
    telefono				String
    tipoSangre				TipoDeSangre
    mail					String
    centroMedico			String
}

entity VisitasPaciente{
	hora 					Instant
    duracion				Double
}

enum TipoDeSangre{
	O_NEGATIVO
    O_POSITIVO
    A_NEGATIVO
    A_POSITIVO
    B_NEGATIVO
    B_POSITIVO
    AB_NEGATIVO
    AB_POSITIVO
}

relationship OneToMany {
	Hospital{salas} to Sala{hospital}
    Sala{pacientes} to Usuario
    Sala{horario} to Horario
}

relationship ManyToOne{
	VisitasPaciente{Paciente} to Usuario
    VisitasPaciente{Visitante} to Usuario
    VisitasPaciente{Sala} to Sala
}

paginate * with pagination

dto * with mapstruct

service all with serviceImpl

angularSuffix * with vitas
